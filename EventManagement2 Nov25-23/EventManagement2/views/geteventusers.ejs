<!DOCTYPE html>
<html>
  <head>
    <title>Registered Users</title>
	<link rel = "stylesheet" type = "text/css" href = "stylesheets/style.css">
	<meta name="viewport" content="width=device-width, initial-scale=1">

  </head>
		<body>

				<div class="nav">
				<a href= "adminhome">Home</a>
				<a href= "admingallery">Gallery</a>
				<a href="adminservices">Services</a>
				<a href="getbookings">Booking</a>
				<a href="geteventusers">Users</a>
				<a href="getfeedback">Feedback</a>
				<a href="getqueries">Queries</a>
				<a href="login"class = "btnn" >Logout</a>
				<h3><span>'Admin Login'</span></h3>
				</div>

                <header>
                    List of Login Users
            </header> 
            
                    <div id="divGet">
                    <table>
                           <tr>
                                   <th class="id2">ID</th>
                                   <th class="id2">First Name</th>
                                   <th class="id2">Last Name</th>
                                   <th class="id2">Email</th>
                                   <th class="id2">Username</th>
                                   <th class="id2">Contact Number</th>
                                   <th class="id2">Password</th>
                                   <th class="id2">Verified Email</th>
                                   <th class="id2">Active</th>
                                   <th class="id2">Registered On</th>
                                   <th class="id2">Verified On</th>
                                   <th class="id2">User Type</th>
                                   <th class="id2" class="table-danger">Edit</th>
                                   <th class="id2" class="table-info">Delete</th>
                            </tr>      
                            <% if (eventusersData.length !=0) {var i=1; eventusersData.forEach( function(data) {
                             %>   
                             <tr>
                                    <td class="login2"><%=data.id %></td>
                                    <td class="login2"><%=data.firstname %></td>
                                    <td class="login2"><%=data.lastname %></td>
                                    <td class="login2"><%=data.email %></td>
                                    <td class="login2"><%=data.username %></td>
                                    <td class="login2"><%=data.contactno%></td>
                                    <td class="login2"><%=data.password %></td>
                                    <td class="login2"><%=data.verifiedemail %></td>
                                    <td class="login2"><%=data.active %></td>
                                    <td class="login2"><%=data.registeredon %></td>
                                    <td class="login2"><%=data.verifiedon %></td>
                                    <td class="login2"><%=data.usertype %></td>
                                    <td class="login2"><a href="">EDIT</a></td>
                                    <td class="login2"><button>Delete</button></td>
                              </tr>
                              <% i++; }) %> <%}%>
            
                        </table>    
                        </div>
            
        </body>
 </html>               